-recurso = prueba.recurso
.prueba(id='prueba[#{prueba.posicion}]' class='#{recurso.type.downcase}')
  .descripcion
    =prueba.descripcion
    
  .left
    -@type = 'prueba'
    -@votable = prueba
    .votar= render 'social/votar'
  
  =link_to(recurso.link, recurso.link) if prueba.link
  -if recurso.file
    %a(href="#{recurso.file.url}" target="_blank") descargar archivo
  -if recurso.embed
    =raw recurso.embed